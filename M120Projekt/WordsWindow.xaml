<Window x:Class="M120Projekt.WordsWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:M120Projekt"
        WindowStartupLocation="CenterScreen"
        mc:Ignorable="d"
        Title="World of Cara - Übersicht Wörter" Height="380" Width="525" Icon="pack://siteoforigin:,,,/Resources/object_leaf.ico">
    <Grid Height="380" Width="525">
    <Grid x:Name="grdContent">

    </Grid>
        <Button x:Name="btnMyWords" Content="Meine Wörter" HorizontalAlignment="Left" Margin="320,320,0,0" VerticalAlignment="Top" Width="85" Click="BtnMyWords_Click" />
        <Button x:Name="btnAllWords" IsEnabled="False" Content="Alle Wörter" HorizontalAlignment="Left" Margin="215,320,0,0" VerticalAlignment="Top" Width="85" Click="BtnAllWords_Click" />
        <Button x:Name="btnBack" Content="Zurück" HorizontalAlignment="Left" Margin="425,320,0,0" VerticalAlignment="Top" Width="75" Click="BtnBack_Click"/>
    </Grid>
</Window>
